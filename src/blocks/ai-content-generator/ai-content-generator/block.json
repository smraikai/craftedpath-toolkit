{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "craftedpath-toolkit/ai-content-generator",
	"version": "0.1.0",
	"title": "AI Content Generator",
	"category": "text",
	"icon": "edit-write",
	"description": "Generates content (paragraphs, headings, etc.) based on your prompt using AI.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"prompt": {
			"type": "string",
			"source": "attribute",
			"selector": "div",
			"attribute": "data-prompt",
			"default": ""
		}
	},
	"textdomain": "ai-content-generator",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}